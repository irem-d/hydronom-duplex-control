{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Command",
  "type": "object",
  "required": [
    "timestamp",
    "vehicle_id",
    "command"
  ],
  "properties": {
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "vehicle_id": {
      "type": "string"
    },
    "command": {
      "enum": [
        "ARM",
        "DISARM",
        "SET_THRUSTERS",
        "SET_RUDDER",
        "SET_BALLAST",
        "SET_WAYPOINTS",
        "START_MISSION",
        "PAUSE_MISSION",
        "ABORT_MISSION",
        "SET_MODE"
      ]
    },
    "payload": {
      "type": [
        "object",
        "null"
      ]
    }
  }
}